generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Driver {
  id          Int    @id @default(autoincrement())
  name        String
  description String
  vehicle     String
  review      String
  value       Int
  minimumKm   Int

  @@map("drivers")
}